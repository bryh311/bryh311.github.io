---
import Post from './Post.astro';

interface Props {
    key: string;
    posts: any[];
}

const {key, posts} = Astro.props;
---
<h3>{key}</h3>
<ul>
    {
        posts.map((post: any) => (
        <Post url={post.url} title={post.frontmatter.title} date={post.frontmatter.date} />
        ))
    }
</ul>